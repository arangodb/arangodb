{
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": false,
      "globalReturn": true
    },
    "sourceType": "script"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "accessor-pairs": 2,
    "arrow-spacing": [2, { "before": true, "after": true }],
    "comma-style": [2, "last"],
    "constructor-super": 2,
    "eqeqeq": [2, "allow-null"],
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-imports": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-parens": [2, "functions"],
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": [2, { "allowLoop": false, "allowSwitch": false }],
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": [2, "except-parens"],
    "no-self-assign": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unmodified-loop-condition": 2,
    "no-unsafe-finally": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-with": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "any"],
    "semi": [2, "always"],
    "spaced-comment": 1,
    "space-unary-ops": 1,
    "no-eval": 1,
    "camelcase": 1,
    "brace-style": 1,
    "no-multiple-empty-lines": 1,
    "space-before-function-paren": 1,
    "no-multi-spaces": 1,
    "no-trailing-spaces": 1,
    "key-spacing": 1,
    "yoda": 1,
    "no-throw-literal": 1,
    "operator-linebreak": 1,
    "no-unused-vars": 1,
    "comma-spacing": 1,
    "block-spacing": 1,
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "no-spaced-func": 1,
    "new-cap": 1,
    "no-new-func": 1,
    "keyword-spacing": 1,
    "curly": 1,
    "no-extra-semi": 1,
    "space-before-blocks": 1,
    "no-useless-escape": 1,
    "no-control-regex": 1,
    "template-curly-spacing": 1,
    "no-unneeded-ternary": 1,
    "generator-star-spacing": 1,
    "yield-star-spacing": 1,
    "dot-location": 1,
    "eol-last": 1,
    "comma-dangle": 1,
    "no-extra-bind": 1,
    "semi-spacing": 1,
    "no-unreachable": 1,
    "no-self-compare": 1,
    "no-duplicate-case": 1,
    "no-whitespace-before-property": 1,
    "no-unexpected-multiline": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-extra-boolean-cast": 1,
    "handle-callback-err": 1,
    "object-property-newline": 1,
    "object-curly-spacing": 0,
    "standard/no-callback-literal": 0,
    "guard-for-in": "off",
    "one-var": "off",
    "quotes": "off",
    "padded-blocks": "off",
    "indent": "off"
  }
}
