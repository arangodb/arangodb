{
  "gitbook": "^3.2.2",
  "title": "ArangoDB Cookbook",
  "author": "ArangoDB GmbH",
  "description": "Recipes for ArangoDB - the native multi-model NoSQL database",
  "language": "en",
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "toggle-chapters",
    "addcssjs",
    "anchorjs",
    "sitemap-general@git+https://github.com/Simran-B/gitbook-plugin-sitemap-general.git",
    "ga",
    "localized-footer",
    "callouts@git+https://github.com/Simran-B/gitbook-plugin-callouts.git",
    "arangodb-outdated@git+https://github.com/Simran-B/gitbook-plugin-arangodb-outdated.git"
  ],
  "pdf": {
    "fontSize": 12,
    "toc": true,
    "margin": {
      "right": 60,
      "left": 60,
      "top": 35,
      "bottom": 35
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "pluginsConfig": {
    "addcssjs": {
      "js": ["styles/header.js"],
      "css": ["styles/header.css"]
    },
    "sitemap-general": {
      "prefix": "https://docs.arangodb.com/3.0/cookbook/",
      "changefreq": "monthly",
      "priority": 0.1
    },
    "ga": {
      "token": "UA-81053435-2"
    },
    "localized-footer": {
      "filename": "FOOTER.html"
    }
  }
}
