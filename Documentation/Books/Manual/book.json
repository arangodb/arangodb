{
  "gitbook": "^3.2.2",
  "title": "ArangoDB VERSION_NUMBER Documentation",
  "author": "ArangoDB GmbH",
  "description": "Official manual for ArangoDB - the native multi-model NoSQL database",
  "language": "en",
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "toggle-chapters",
    "addcssjs",
    "anchorjs",
    "sitemap-general@git+https://github.com/Simran-B/gitbook-plugin-sitemap-general.git",
    "ga",
    "callouts@git+https://github.com/Simran-B/gitbook-plugin-callouts.git",
    "edit-link@git+https://github.com/Simran-B/gitbook-plugin-edit-link.git",
    "localized-footer"
  ],
  "pdf": {
    "fontSize": 12,
    "toc": true,
    "margin": {
      "right": 60,
      "left": 60,
      "top": 35,
      "bottom": 35
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "pluginsConfig": {
    "addcssjs": {
      "js": ["styles/header.js"],
      "css": ["styles/header.css"]
    },
    "sitemap-general": {
      "prefix": "https://docs.arangodb.com/3.1/Manual/",
      "changefreq": "weekly",
      "priority": 0.8
    },
    "ga": {
      "token": "UA-81053435-2"
    },
    "localized-footer": {
      "filename": "FOOTER.html"
    },
    "edit-link": {
      "base": "https://github.com/arangodb/arangodb/edit/3.1/Documentation/Books/Manual",
      "label": "Edit Page",
      "extension": ".mdpp"
    }
  }
}
