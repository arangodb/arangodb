	api-collection-spec.rb \
	rest-create-document-spec.rb \
	rest-read-document-spec.rb \
	rest-update-document-spec.rb \
	rest-delete-document-spec.rb \
	rest-edge-spec.rb \
	api-index-spec.rb \
	api-index-hash-spec.rb \
	api-index-skiplist-spec.rb \
        api-cursor-spec.rb \
        api-simple-spec.rb
