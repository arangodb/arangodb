        <name>buffer_side_selector</name>
        <enumvalue id="group__enum_1ggaf73238cbd03ff9961f8547d084d83e2ca85f7bc4b2bf45e6ce03256a4edac146f" prot="public">
          <name>buffer_side_left</name>
