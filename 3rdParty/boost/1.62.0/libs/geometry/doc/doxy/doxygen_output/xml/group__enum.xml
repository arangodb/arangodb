        <enumvalue id="group__enum_1gga0af1c124acb76f0892024d6d4ab2bc8ca29458b1c5c6f01e41bc529cff7cdbec2" prot="public">
          <name>clockwise</name>
          <initializer>= 1</initializer>
          <briefdescription>
<para>Points are ordered clockwise. </para>          </briefdescription>
          <detaileddescription>
