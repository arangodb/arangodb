then
    REQUIRE_LIB_DEPS=no
else
    REQUIRE_LIB_DEPS=yes
fi

 if test x$REQUIRE_LIB_DEPS = xyes; then
  USE_EXPLICIT_LIB_DEPS_TRUE=
  USE_EXPLICIT_LIB_DEPS_FALSE='#'
else
  USE_EXPLICIT_LIB_DEPS_TRUE='#'
  USE_EXPLICIT_LIB_DEPS_FALSE=
fi


