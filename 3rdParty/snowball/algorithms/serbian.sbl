            '{cyrO}'    (<- 'o')
            '{cyrP}'    (<- 'p')
            '{cyrR}'    (<- 'r')
            '{cyrS}'    (<- 's')
            '{cyrT}'    (<- 't')
            '{cyrCy}'   (<- '{cy}')
            '{cyrU}'    (<- 'u')
            '{cyrF}'    (<- 'f')
            '{cyrH}'    (<- 'h')
            '{cyrC}'    (<- 'c')
            '{cyrCx}'   (<- '{cx}')
            '{cyrDzx}'  (<- 'd{zx}')
            '{cyrSx}'   (<- '{sx}')
        )
    )

)

define prelude as (

    do repeat goto (
        ca ['ije'] ca <- 'e'
    )

    do repeat goto (
        ca ['je'] ca <- 'e'
    )

    do repeat goto (
        ['dj'] <- '{dx}'
    )

)

define mark_regions as (

    set no_diacritics

    do (
        gopast sa unset no_diacritics
    )

    $p1 = limit

    do (
        gopast v setmark p1
        ($p1 < 2) (
            gopast non-v
            setmark p1
        )
    )
    do (
        gopast 'r'
        $(cursor >= 2) or (gopast non-rg)
        $(p1 - cursor > 1) setmark p1
    )

)

